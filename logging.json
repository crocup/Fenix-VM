{  
  "version": 1,  
  "disable_existing_loggers": false,
  "formatters": {  
    "simple": {  
      "format": "[%(levelname)s]: %(name)s: [%(asctime)s] - %(message)s"
  }  
  },  
  "handlers": {  
    "log_json": {
      "class": "logging.handlers.RotatingFileHandler",
      "level": "DEBUG",
      "formatter": "simple",
      "filename": "logs/logging.log",
      "maxBytes": 4194304,
      "backupCount": 5,
      "encoding": "utf8"
  }
  },  
  "loggers": {
        "app": {
            "level": "DEBUG",
            "handlers": ["log_json"]
        }
    }
}